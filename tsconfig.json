{
    "compilerOptions": {
      "target": "ES2019",
      "module": "commonjs",
      "outDir": "dist",
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "strict": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "sourceMap": true
    },
    "include": ["src"],
    "exclude": ["node_modules", "dist"]
  }
  